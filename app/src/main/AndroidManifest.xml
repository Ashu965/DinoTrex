<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.DinoTrex">

    <application
        android:name="com.example.DinoTrex.App"
        android:allowBackup="true"
        android:icon="@mipmap/th"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RunningGame">
        <activity
            android:name="com.example.DinoTrex.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.RunningGame.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>